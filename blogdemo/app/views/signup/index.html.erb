<%= form_for :user, url:'/signup/create' do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p

  <p>
    <%= f.label    "RePassword" %><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Sign Up" %>
  </p>
<% end %>